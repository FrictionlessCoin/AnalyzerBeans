<?xml version="1.0" encoding="UTF-8"?>
<job xmlns="http://eobjects.org/analyzerbeans/job/1.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">

	<source>
		<data-context ref="my database" />
		<columns>
			<column id="col_prod" path="product" />
			<column id="col_version" path="version" />
		</columns>
	</source>

	<transformation>
		<transformer>
			<descriptor ref="Dictionary matcher" />
			<properties>
				<property name="Dictionaries"
					value="[eobjects.org products,apache products,logging products]" />
			</properties>
			<input ref="col_prod" />
			<output id="trans-1" name="eobjects match" />
			<output id="trans-2" name="apache match" />
			<output id="trans-3" name="logging match" />
		</transformer>
		
		<transformer>
			<descriptor ref="Convert to number"/>
			<input ref="trans-3"/>
			<output id="trans-4" name="logging match -> number"/>
		</transformer>
	</transformation>

	<analysis>
		<analyzer>
			<descriptor ref="Value distribution" />
			<input ref="trans-1" />
		</analyzer>
		<analyzer>
			<descriptor ref="Value distribution" />
			<input ref="trans-2" />
		</analyzer>
		<analyzer>
			<descriptor ref="Value distribution" />
			<input ref="trans-3" />
		</analyzer>
		<analyzer>
			<descriptor ref="Value distribution" />
			<input ref="trans-4" />
		</analyzer>
	</analysis>

</job>